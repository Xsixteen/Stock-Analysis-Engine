# Charles Schwab API Credentials
# Get these from https://developer.schwab.com/
SCHWAB_API_KEY=your_app_key_here
SCHWAB_APP_SECRET=your_app_secret_here
SCHWAB_CALLBACK_URL=https://127.0.0.1:8182/
SCHWAB_TOKEN_PATH=./schwab_token.json
SCHWAB_ACCOUNT_ID=0

# OpenAI API Key (for GPT-4 news analysis)
OPENAI_API_KEY=sk-your-openai-api-key

# Finnhub API Key (for news headlines)
FINNHUB_API_KEY=your_finnhub_api_key

# Gemini API Configuration
GEMINI_API_KEY=gemini-key
# Model to use (default: gemini-2.5-pro). Other options: gemini-2.5-flash, gemini-1.5-pro
GEMINI_MODEL=gemini-2.5-pro
# Delay in seconds between Gemini API calls (default: 30 for 2 requests per minute)
GEMINI_API_DELAY=30